; dune file generated by '%{bin:myconf} fmt gen-dune' -- do not edit.

(rule
 (with-stdout-to
  comments.myconf.pp.output
  (run %{bin:myconf} fmt file %{dep:comments.myconf})))

(rule
 (alias fmt)
 (action
  (diff comments.myconf comments.myconf.pp.output)))

(rule
 (with-stdout-to
  simple.myconf.pp.output
  (run %{bin:myconf} fmt file %{dep:simple.myconf})))

(rule
 (alias fmt)
 (action
  (diff simple.myconf simple.myconf.pp.output)))
